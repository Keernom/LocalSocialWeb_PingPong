<Page x:Class="Kursovik_Kocherzhenko.View.UserSearchPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Kursovik_Kocherzhenko.View"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="UserSearchPage">

    <Grid>
        <ScrollViewer VerticalScrollBarVisibility="Hidden"
                      CanContentScroll="False">
            <StackPanel Name="UserFindResult" HorizontalAlignment="Stretch"
                       VerticalAlignment="Top" Orientation="Vertical">
            </StackPanel>
        </ScrollViewer>
    </Grid>
</Page>
